{
    "name": "buzzingpixel/corbomite-di",
    "description": "Corbomite DI",
    "version": "1.2.0",
    "license": "Apache-2.0",
    "homepage": "https://buzzingpixel.com/",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "buzzingpixel/corbomite-config-collector": "^1.1",
        "php-di/php-di": "^6.0"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\di\\": "src/"
        }
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
