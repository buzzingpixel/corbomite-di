{
    "name": "buzzingpixel/corbomite-di",
    "description": "Corbomite DI",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "homepage": "https://buzzingpixel.com/",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "php-di/php-di": "^6.0",
        "composer/composer": "^1.8"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\di\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
